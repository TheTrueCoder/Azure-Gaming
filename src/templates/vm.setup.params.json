{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "australiaeast"
        },
        "networkInterfaceName": {
            "value": "ncloudvm-nic"
        },
        "networkSecurityGroupName": {
            "value": "NCloudVM-nsg"
        },
        "networkSecurityGroupRules": {
            "value": [
                {
                    "name": "RDP",
                    "properties": {
                        "priority": 300,
                        "protocol": "TCP",
                        "access": "Allow",
                        "direction": "Inbound",
                        "sourceAddressPrefix": "*",
                        "sourcePortRange": "*",
                        "destinationAddressPrefix": "*",
                        "destinationPortRange": "3389"
                    }
                }
            ]
        },
        "subnetName": {
            "value": "default"
        },
        "virtualNetworkName": {
            "value": "NCloudVM_group-vnet"
        },
        "publicIpAddressName": {
            "value": "NCloudVM-ip"
        },
        "virtualMachineName": {
            "value": "NCloudVM"
        },
        "virtualMachineRG": {
            "value": "NCloudVM_group"
        },
        "osDiskType": {
            "value": "StandardSSD_LRS"
        },
        "virtualMachineSize": {
            "value": "Standard_B2ms"
        },
        "adminUsername": {
            "value": "Username"
        },
        "adminPassword": {
            "value": null
        }
    }
}